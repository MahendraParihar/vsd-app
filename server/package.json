  "scripts": {
    "build_shared_lib": "npm --prefix ../shared-library run build",
    "dev_shared_lib": "npm --prefix ../shared-library run dev",
    "start": "concurrently \"npm run dev_shared_lib\" \"nx run-many --target=serve --maxParallel=10 --projects=main,event,family,job,mandal,matrimonial,news,temple,lovs,facility\"",
