{
  "name": "vsd-admin",
  "version": "1.0.0",
  "license": "Private",
  "scripts": {
    "build_shared_lib": "npm --prefix ../shared-library run build",
    "dev_shared_lib": "npm --prefix ../shared-library run dev",
    "start": "npm run build_shared_lib && nx serve",
    "dev": "nx serve main",
    "dev:main": "nx serve main",
    "dev_all": "concurrently \"npm run dev_shared_lib\"  \"npm run dev\"",
    "build": "npm run build_shared_lib && nx run-many --target=build --projects=main && nx run-many --target=build --projects=event,news,temple,mandal,family,matrimonial,event,lov,job,facility",
    "postinstall": "mkdir -p .nx"
  },
