{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@vsd-common/lib": ["../shared-library/dist"],
      "@vsd-frontend/core-lib": ["libs/core-lib/src/index.ts"],
      "@vsd-frontend/event-lib": ["libs/event-lib/src/index.ts"],
      "@vsd-frontend/family-lib": ["libs/family-lib/src/index.ts"],
      "@vsd-frontend/job-lib": ["libs/job-lib/src/index.ts"],
      "@vsd-frontend/lovs-lib": ["libs/lovs-lib/src/index.ts"],
      "@vsd-frontend/mandal-lib": ["libs/mandal-lib/src/index.ts"],
      "@vsd-frontend/metrimonial-lib": ["libs/metrimonial-lib/src/index.ts"],
      "@vsd-frontend/news-lib": ["libs/news-lib/src/index.ts"],
      "@vsd-frontend/shared-ui-lib": ["libs/shared-ui-lib/src/index.ts"],
      "@vsd-frontend/temple-lib": ["libs/temple-lib/src/index.ts"],
      "@vsd-frontend/facility-lib": ["libs/facility-lib/src/index.ts"],
      "event/Module": ["apps/event/src/app/remote-entry/entry.module.ts"],
      "facility/Module": ["apps/facility/src/app/remote-entry/entry.module.ts"],
      "family/Module": ["apps/family/src/app/remote-entry/entry.module.ts"],
      "job/Module": ["apps/job/src/app/remote-entry/entry.module.ts"],
      "lov/Module": ["apps/lov/src/app/remote-entry/entry.module.ts"],
      "mandal/Module": ["apps/mandal/src/app/remote-entry/entry.module.ts"],
      "matrimonial/Module": ["apps/matrimonial/src/app/remote-entry/entry.module.ts"],
      "news/Module": ["apps/news/src/app/remote-entry/entry.module.ts"],
      "temple/Module": ["apps/temple/src/app/remote-entry/entry.module.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"],
  "references": [
    {
      "path": "../shared-library"
    }
  ]
}
